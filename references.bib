  issn = {1461-0248},
  doi = {10.1046/j.1461-0248.2003.00469.x},
  abstract = {Studies mostly focused on communities of primary producers have shown that species richness provides and promotes fundamental ecosystem services. However, we know very little about the factors influencing ecosystem services provided by higher trophic levels in natural food webs. Here we present evidence that differences in food web structure and the richness of herbivores in 19 plant-herbivore-parasitoid food webs influence the service supplied by natural enemies, namely, the parasitism rates on hosts. Specifically, we find that parasitoids function better in simple food webs than in complex ones, a result relevant to biological control practice. More generally, we show that species richness per se only contributes partially to the understanding of higher-level ecosystem services in multitrophic communities, and that changes in food web complexity should also be taken into account when predicting the effects of human-driven disturbances in natural communities.},
  langid = {english},
  keywords = {Biodiversity,clustering,ecosystem function,food web structure,higher-level interactions,indirect effects,omnivory,parasitoids},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1046/j.1461-0248.2003.00469.x},
  file = {/home/benobo/Zotero/storage/I2WVE9S4/Montoya et al. - 2003 - Food web complexity and higher-level ecosystem ser.pdf;/home/benobo/Zotero/storage/NHJE877R/j.1461-0248.2003.00469.html}
}

@article{Montoya2009PrePer,
  title = {Press Perturbations and Indirect Effects in Real Food Webs},
  author = {Montoya, Jos{\'e}M and Woodward, Guy and Emmerson, Mark C. and Sol{\'e}, Ricard V.},
  year = {2009},
  journal = {Ecology},
  volume = {90},
  number = {9},
  pages = {2426--2433},
  issn = {1939-9170},
  doi = {10.1890/08-0657.1},
  abstract = {The prediction of the effects of disturbances in natural systems is limited by the general lack of knowledge on the strength of species interactions, i.e., the effect of one species on the population growth rate of another, and by the uncertainty of the effects that may be manifested via indirect pathways within the food web. Here we explored the consequences of changes in species populations for the remaining species within nine exceptionally well-characterized empirical food webs, for which, unlike the vast majority of other published webs, feeding links have been fully quantified. Using the inverse of the Jacobian matrix, we found that perturbations to species with few connections have larger net effects (considering both direct and indirect pathways between two species) on the rest of the food web than do disturbances to species that are highly connected. For 40\% of predator\textendash prey links, predators had positive net effects on prey populations, due to the predominance of indirect interactions. Our results highlight the fundamental, but often counterintuitive, role of indirect effects for the maintenance of food web complexity and biodiversity.},
  copyright = {\textcopyright{} 2009 by the Ecological Society of America},
  langid = {english},
  keywords = {connectance,food webs,indirect effects,inverse Jacobian matrix,linkage density,press perturbation,specialization},
  annotation = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.1890/08-0657.1},
  file = {/home/benobo/Zotero/storage/WLDWRLY2/Montoya et al. - 2009 - Press perturbations and indirect effects in real f.pdf}
}

